<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
  Launch demo modal
</button>

<div class="modal fade modal-dialog" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="exampleModalLabel">Modal title</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <%= simple_form_for([@location, @feedback]) do |f| %>
        <h1>One Vibing Photo, please.</h1>
           <%= f.input :photos, as: :file, input_html: { multiple: true }%>
            <h1>One Vibing Video, please.</h1>
           <%= f.input :videos, as: :file, input_html: { multiple: true }%>
            <h1>What do you think about place vibe?</h1>
           <%= f.input :comment, label: "What do you think about place vibe?"%>
            <h1>give a rate for it!</h1>
           <%= f.input :rating, label: "give a rate for it!"%>
           <%= f.submit %>
           <% end %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

